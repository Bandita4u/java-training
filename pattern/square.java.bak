import java.util.*;
class square
	{
	public static void main(String[] args) 
	{
		Scanner a=new Scanner(System.in);
		 System.out.println("enter a num");
		int n=a.nextInt();
		
		for (int i=1 ;i<=n;i++ )
		{
			for(int j=1;j<=n;j++)
			{
				if(i==1||i==n) {
					System.out.print("X");
				}
				else{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}